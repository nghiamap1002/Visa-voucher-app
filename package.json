{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_ENV=dev react-scripts start",
    "start:staging": "REACT_APP_ENV=staging react-scripts start",
    "start:prod": "REACT_APP_ENV=prod react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "pretty": "prettier --write --config .prettierrc src --ext .ts,.tsx,.js,.jsx",
    "lint:js": "eslint src --ext .tsx,.ts,.js,.jsx",
    "format:js": "yarn lint:js -- --fix"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^2.9.6",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/styled-engine-sc": "^5.8.0",
    "@mui/styles": "^5.8.7",
    "@mui/system": "^5.11.4",
    "@mui/x-date-pickers": "^5.0.2",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/node": "^17.0.45",
    "@types/react-dom": "^18.0.6",
    "axios": "^0.27.2",
    "framer-motion": "^6.5.1",
    "lint-staged": "^13.1.0",
    "moment": "^2.29.4",
    "numeral": "^2.0.6",
    "prettier": "^2.8.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-i18next": "^11.18.3",
    "react-number-format": "^5.1.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.7",
    "redux": "^4.2.0",
    "redux-deep-persist": "^1.0.7",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-ts": "^0.4.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "4",
    "source-map-explorer": "^2.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
